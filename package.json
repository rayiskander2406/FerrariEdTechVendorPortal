{
  "name": "temp-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:generated": "vitest run tests/generated/",
    "typecheck": "tsc --noEmit",
    "pii-check": "./scripts/pii-check.sh",
    "pre-commit": "./scripts/pre-commit-check.sh",
    "quality": "npm run typecheck && npm run pii-check && npm run lint",
    "validate-demo": "npx tsx scripts/validate-demo.ts",
    "demo-ready": "npm run quality && npm run validate-demo",
    "changelog": "./scripts/generate-changelog.sh",
    "generate:spec": "npx tsx spec/generator/index.ts",
    "verify:spec": "npm run generate:spec && git diff --exit-code tests/generated/",
    "db:seed": "npx tsx lib/db/seed.ts",
    "db:seed:clear": "npx tsx lib/db/seed.ts --clear",
    "db:seed:reseed": "npx tsx lib/db/seed.ts --reseed"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.29.0",
    "@upstash/redis": "^1.35.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "next": "14.2.33",
    "pino": "^10.1.0",
    "prom-client": "^15.1.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@prisma/client": "^5.22.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "fast-check": "^4.3.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.14"
  }
}
